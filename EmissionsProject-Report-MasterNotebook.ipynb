    "                          orientation='horizontal',\n",
    "                          readout=True,\n",
    "                          readout_format='d'))\n",
    "def update_fig(numComp):\n",
    "    fig3=go.Figure(layout=layout3)\n",
    "    for i in range(numComp):\n",
    "        fig3.add_scatter(x=x,\n",
    "                      y=df_fig3.loc[i][['2011', '2012', '2013', '2014', '2015', '2016', '2017', '2018']],\n",
    "                      mode='lines+markers',\n",
    "                      name=df_fig3.loc[i]['COMPANY']\n",
    "                     )\n",
    "    fig3.show()\n"
